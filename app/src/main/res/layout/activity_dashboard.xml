<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ActivitesCollection.DashboardActivity">


    <FrameLayout
        android:id="@+id/collectionfragmentsReplacer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1" />

    <com.ismaeldivita.chipnavigation.ChipNavigationBar
        android:id="@+id/menuChipNavigationBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#1BABE4"
        android:padding="6dp"
        app:cnb_badgeColor="#178767"
        app:cnb_iconSize="22dp"
        app:cnb_menuResource="@menu/menudetect"
        app:cnb_orientationMode="horizontal"
        app:cnb_radius="15dp"
        app:cnb_unselectedColor="#FFFFFF" />

</LinearLayout>