<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <corners
        android:radius="10dp"/>

    <solid android:color="#1BABE4"/>

    <padding
        android:bottom="5dp"
        android:top="5dp"
        android:left="5dp"
        android:right="5dp"/>

    <stroke
        android:dashGap="3dp"
        android:color="#2CEFC1"
        android:dashWidth="3dp"
        android:width="1dp"/>

</shape>